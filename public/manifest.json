{
  "manifest_version": 3,
  "name": "Cookie Copier",
  "version": "1.0",
  "description": "Copy cookie fields between domains",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/icon_16.png",
      "48": "assets/icon_48.png",
      "128": "assets/icon_128.png"
    }
  },
  "icons": {
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}